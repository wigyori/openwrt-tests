config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd5b:6d7d:168d::/48'
	option packet_steering '1'

config device 'switch'
	option name 'switch'
	option type 'bridge'
	option macaddr 'd8:ec:e5:72:d1:a7'
	list ports 'lan1'
	list ports 'lan2'
	list ports 'lan3'
	list ports 'lan4'
	list ports 'lan5'
	list ports 'lan6'
	list ports 'lan7'
	list ports 'lan8'
	list ports 'lan9'
	list ports 'lan10'

config bridge-vlan 'lan_vlan'
	option device 'switch'
	option vlan '1'
	list ports 'lan10'

config device
	option name 'switch.1'
	option macaddr 'd8:ec:e5:72:d1:a7'

config interface 'lan'
	option device 'switch.1'
	option proto 'static'
	option ip6assign '60'
	list ipaddr '192.168.128.2/24'

config bridge-vlan
	option device 'switch'
	option vlan '101'
	list ports 'lan1'
	list ports 'lan10:t'

config bridge-vlan
	option device 'switch'
	option vlan '102'
	list ports 'lan2'
	list ports 'lan10:t'

config bridge-vlan
	option device 'switch'
	option vlan '103'
	list ports 'lan3'
        list ports 'lan10:t'

config bridge-vlan
	option device 'switch'
	option vlan '104'
	list ports 'lan4'
	list ports 'lan10:t'

config bridge-vlan
	option device 'switch'
	option vlan '105'
	list ports 'lan5'
        list ports 'lan10:t'

config bridge-vlan
	option device 'switch'
	option vlan '106'
	list ports 'lan6'
	list ports 'lan10:t'

config bridge-vlan
        option device 'switch'
        option vlan '107'
        list ports 'lan7'
        list ports 'lan10:t'

config bridge-vlan
        option device 'switch'
        option vlan '108'
        list ports 'lan8'
        list ports 'lan10:t'
		